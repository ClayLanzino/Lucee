{
	"version":"4.2"
	,"system":{
		"out":"system"
		,"err":"system"
	}
	,"data-sources":{
		"preserve-single-quote":"yes"
	}
	,"mode":{
		"develop":"false"
	}
	,"file-system":{
		"temp-directory":"{lucee-config}/temp/"
		,"fld-default-directory":"{lucee-config}/library/fld/"
		,"tld-default-directory":"{lucee-config}/library/tld/"
		,"function-default-directory":"{lucee-config}/library/function/"
		,"tag-default-directory":"{lucee-config}/library/tag/"
	}
	,"dump-writers":{
		"dump-writer":[
			{
				"name":"html"
				,"class":"lucee.runtime.dump.HTMLDumpWriter"
				,"default":"browser"
			}
			,{
				"name":"text"
				,"class":"lucee.runtime.dump.TextDumpWriter"
				,"default":"console"
			}
			,{
				"name":"classic"
				,"class":"lucee.runtime.dump.ClassicHTMLDumpWriter"
			}
			,{
				"name":"simple"
				,"class":"lucee.runtime.dump.SimpleHTMLDumpWriter"
			}
		]
	}
	,"remote-clients":{
		"directory":"{lucee-config}remote-client/"
	}
	,"resources":{
		"default-resource-provider":{
			"class":"lucee.commons.io.res.type.file.FileResourceProvider"
			,"arguments":"lock-timeout:1000;"
		}
		,"resource-provider":[
			{
				"scheme":"ftp"
				,"class":"lucee.commons.io.res.type.ftp.FTPResourceProvider"
				,"arguments":"lock-timeout:20000;socket-timeout:-1;client-timeout:60000"
			}
			,{
				"scheme":"zip"
				,"class":"lucee.commons.io.res.type.zip.ZipResourceProvider"
				,"arguments":"lock-timeout:1000;case-sensitive:true;"
			}
			,{
				"scheme":"tar"
				,"class":"lucee.commons.io.res.type.tar.TarResourceProvider"
				,"arguments":"lock-timeout:1000;case-sensitive:true;"
			}
			,{
				"scheme":"tgz"
				,"class":"lucee.commons.io.res.type.tgz.TGZResourceProvider"
				,"arguments":"lock-timeout:1000;case-sensitive:true;"
			}
			,{
				"scheme":"http"
				,"class":"lucee.commons.io.res.type.http.HTTPResourceProvider"
				,"arguments":"lock-timeout:10000;case-sensitive:false;"
			}
			,{
				"scheme":"https"
				,"class":"lucee.commons.io.res.type.http.HTTPSResourceProvider"
				,"arguments":"lock-timeout:10000;case-sensitive:false;"
			}
			,{
				"scheme":"s3"
				,"class":"lucee.commons.io.res.type.s3.S3ResourceProvider"
				,"arguments":"lock-timeout:10000;"
			}
		]
	}
	,"scope":{
		"cascading":"standard"
		,"cascade-to-resultset":"yes"
		,"merge-url-form":"no"
		,"sessiontimeout":"0,0,30,0"
		,"applicationtimeout":"1,0,0,0"
		,"requesttimeout":"0,0,0,50"
		,"sessionmanagement":"yes"
		,"clientmanagement":"no"
		,"setclientcookies":"yes"
		,"setdomaincookies":"no"
		,"client-directory-max-size":"10mb"
		,"client-max-age":"90"
	}
	,"mail":{
		"spool-interval":"5"
		,"spool-enable":"yes"
		,"timeout":"30"
	}
	,"mappings":{
		"mapping":[
			{
				"readonly":"yes"
				,"virtual":"/lucee-server/"
				,"physical":"{lucee-server}/context/"
				,"archive":""
				,"primary":"physical"
				,"listener-mode":"modern"
				,"listener-type":"curr2root"
				,"inspect-template":"once"
			}
			,{
				"readonly":"yes"
				,"virtual":"/lucee/"
				,"physical":"{lucee-config}/context/"
				,"archive":"{lucee-config}/context/lucee-context.lar"
				,"primary":"physical"
				,"listener-mode":"modern"
				,"listener-type":"curr2root"
				,"inspect-template":"once"
			}
		]
	}
	,"custom-tag":{
		"mapping":{
			"inspect-template":"never"
			,"physical":"{lucee-config}/customtags/"
		}
	}
	,"ext-tags":{
		"ext-tag":{
			"type":"java"
			,"name":"HelloWorld"
			,"class":"lucee.cfx.example.HelloWorld"
		}
	}
	,"component":{
		"base-cfml":"/lucee/Component.cfc"
		,"base-lucee":"/lucee/Component.lucee"
		,"data-member-default-access":"public"
		,"dump-template":"/lucee/component-dump.cfm"
		,"mapping":[
			{
				"virtual":"/default"
				,"physical":"{lucee-web}/components/"
				,"primary":"physical"
				,"inspect-template":"never"
			}
			,{
				"virtual":"/default-server"
				,"physical":"{lucee-server}/components/"
				,"primary":"physical"
				,"inspect-template":"never"
			}
		]
	}
	,"cache-handlers":{
		"cache-handler":[
			{
				"id":"request"
				,"class":"lucee.runtime.cache.tag.request.RequestCacheHandler"
			}
			,{
				"id":"timespan"
				,"class":"lucee.runtime.cache.tag.timespan.TimespanCacheHandler"
			}
		]
	}
	,"regional":{
		"timeserver":"pool.ntp.org"
	}
	,"orm":{
		"engine-class":"lucee.runtime.orm.DummyORMEngine"
	}
	,"debugging":{
		"log-memory-usage":"no"
		,"template":"/lucee/templates/debugging/debugging.cfm"
		,"debug":"no"
		,"show-query-usage":"no"
	}
	,"application":{
		"listener-type":"mixed"
		,"listener-mode":"curr2root"
	}
	,"update":{
		"location":"https://update.lucee.org"
		,"type":"manual"
	}
	,"logging":{
		"logger":[
			{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/mapping.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"mapping"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/rest.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"rest"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/gateway.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"gateway"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/remoteclient.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"remoteclient"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/orm.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"orm"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/mail.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"mail"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/search.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"search"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/scheduler.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"scheduler"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/scope.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"scope"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/application.log"
				,"level":"info"
				,"layout":"classic"
				,"name":"application"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/exception.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"exception"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/trace.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"trace"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/thread.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"thread"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/deploy.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"deploy"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/requesttimeout.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"requesttimeout"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/memory.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"memory"
			}
			,{
				"appender":"resource"
				,"appender-arguments":"path:{lucee-config}/logs/datasource.log"
				,"level":"error"
				,"layout":"classic"
				,"name":"datasource"
			}
		]
	}
}